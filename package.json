{
	"name": "gitpaper",
	"description": "ðŸ“° The first ever generator of non-boring and actually detailed release notes",
	"version": "0.0.0",
	"files": [
		"dist/",
		"dist"
	],
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		"./cli/gitpaper": {
			"import": "./dist/cli/gitpaper.mjs"
		},
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			}
		}
	},
	"license": "MIT",
	"author": "Okinea Dev <hi@okinea.dev> (https://github.com/okineadev)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/okineadev/gitpaper.git"
	},
	"funding": "https://github.com/okineadev/gitpaper?sponsor=1",
	"keywords": [
		"changelog",
		"release notes",
		"github"
	],
	"bin": {
		"gitpaper": "./dist/cli/gitpaper.mjs"
	},
	"scripts": {
		"lint:tsc": "tsc --noEmit -p tsconfig.node.json",
		"lint:biome": "biome check",
		"lint:spell": "cspell --gitignore .",
		"lint": "bun run lint:tsc && bun run lint:biome && bun run lint:spell",
		"format": "biome format --write",
		"test": "bun test",
		"preversion": "bun run format && git add .",
		"build": "bunup",
		"dev": "bunup --watch",
		"prepare": "husky"
	},
	"dependencies": {
		"@ai-sdk/google": "^2.0.11",
		"@octokit/rest": "^22.0.0",
		"ai": "^5.0.29",
		"c12": "^3.2.0",
		"commander": "^14.0.0",
		"execa": "^9.6.0",
		"flat-cache": "^6.1.14",
		"handlebars": "^4.7.8",
		"scule": "^1.3.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.2",
		"@types/node": "^24.3.0",
		"bun-types": "^1.2.21",
		"bunup": "0.11.22",
		"cspell": "^9.2.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.5",
		"typescript": "^5.9.2"
	}
}
